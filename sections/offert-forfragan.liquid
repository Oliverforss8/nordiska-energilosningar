{% stylesheet %}
  /* Global fix for Safari tap highlight */
  * {
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0) !important;
    -webkit-tap-highlight-color: transparent !important;
  }

  /* Service option button styling */
  button.service-option {
    transition: all 0.3s ease;
    border: 2px solid #e5e5e5;
    cursor: pointer;
    background: white;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0) !important;
    -webkit-tap-highlight-color: transparent !important;
    -webkit-appearance: none !important;
    -moz-appearance: none !important;
    appearance: none !important;
    outline: none !important;
    user-select: none;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    touch-action: manipulation;
    -webkit-touch-callout: none;
  }

  button.service-option:hover {
    transform: translateY(-2px);
    box-shadow: 0 4px 12px rgba(23, 23, 23, 0.1);
  }

  button.service-option:focus {
    outline: none !important;
    -webkit-tap-highlight-color: transparent !important;
  }

  button.service-option:active {
    transform: translateY(0);
    -webkit-tap-highlight-color: transparent !important;
  }

  /* Selected state - using !important to override everything */
  button.service-option[data-selected='true'] {
    background: linear-gradient(135deg, #ffa94d 0%, #ff4e33 100%) !important;
    border-color: #ff4e33 !important;
    box-shadow: 0 4px 16px rgba(255, 78, 51, 0.3) !important;
    -webkit-tap-highlight-color: transparent !important;
  }

  /* Default state - ensure white background */
  button.service-option[data-selected='false'] {
    background: white !important;
    border-color: #e5e5e5 !important;
    box-shadow: none !important;
    -webkit-tap-highlight-color: transparent !important;
  }

  /* Submit button styling */
  button.quote-submit-btn {
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0) !important;
    -webkit-tap-highlight-color: transparent !important;
    -webkit-appearance: none !important;
    -moz-appearance: none !important;
    appearance: none !important;
    outline: none !important;
    user-select: none;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    touch-action: manipulation;
    border: none;
    -webkit-touch-callout: none;
  }

  button.quote-submit-btn:focus {
    outline: none !important;
    -webkit-tap-highlight-color: transparent !important;
  }

  button.quote-submit-btn:active {
    transform: scale(0.98);
    -webkit-tap-highlight-color: transparent !important;
  }

  /* Fix for inputs and textareas */
  input,
  textarea {
    -webkit-tap-highlight-color: transparent !important;
  }

  /* Prevent SVG elements from capturing tap events */
  button svg,
  button img {
    pointer-events: none;
  }
{% endstylesheet %}

<script src="{{ 'quote-form.js' | asset_url }}" defer></script>

{% schema %}
{
  "name": "Offertförfrågan",
  "tag": "section",
  "class": "section-offert-forfragan",
  "max_blocks": 50,
  "blocks": [
    {
      "type": "service_option",
      "name": "Tjänsteval",
      "limit": 12,
      "settings": [
        {
          "type": "text",
          "id": "label",
          "label": "Etikett",
          "default": "Solceller"
        },
        {
          "type": "text",
          "id": "service_key",
          "label": "Nyckel (valfritt)",
          "info": "Används som data-service. Lämna tomt för att använda etiketten."
        },
        {
          "type": "image_picker",
          "id": "icon_image",
          "label": "Ikonbild (valfritt)"
        },
        {
          "type": "html",
          "id": "icon_svg",
          "label": "Inline SVG (valfritt)",
          "info": "Klistra in rå SVG-kod här om du vill använda egen SVG."
        }
      ]
    },
    {
      "type": "form_field",
      "name": "Formulärfält",
      "limit": 30,
      "settings": [
        { "type": "text", "id": "label", "label": "Etikett", "default": "För- & efternamn" },
        {
          "type": "text",
          "id": "name",
          "label": "Fältnamn (valfritt)",
          "info": "Om tomt används etiketten som namn (handleized)"
        },
        {
          "type": "select",
          "id": "field_type",
          "label": "Typ",
          "options": [
            { "value": "text", "label": "Text" },
            { "value": "email", "label": "E‑post" },
            { "value": "tel", "label": "Telefon" },
            { "value": "textarea", "label": "Textarea" }
          ],
          "default": "text"
        },
        { "type": "text", "id": "placeholder", "label": "Platshållare (valfritt)" },
        { "type": "checkbox", "id": "required", "label": "Obligatoriskt", "default": true },
        {
          "type": "text",
          "id": "pattern",
          "label": "HTML5 pattern (valfritt)",
          "info": "RegExp för validering, t.ex.^[0-9\\- +()]*$ för telefon"
        },
        {
          "type": "number",
          "id": "rows",
          "label": "Textarea rader",
          "default": 4,
          "info": "Gäller endast textarea"
        }
      ]
    }
  ],
  "settings": [
    {
      "type": "header",
      "content": "Innehåll"
    },
    {
      "type": "text",
      "id": "title",
      "label": "Rubrik",
      "default": "Begär offert"
    },
    {
      "type": "richtext",
      "id": "description",
      "label": "Beskrivning",
      "default": "<p>Fyll i formuläret nedan så återkommer vi till dig med en skräddarsydd offert.</p>"
    },
    {
      "type": "header",
      "content": "Kort 1"
    },
    {
      "type": "image_picker",
      "id": "card_1_image",
      "label": "Bild 1"
    },
    {
      "type": "text",
      "id": "card_1_button_text",
      "label": "Knapptext 1"
    },
    {
      "type": "url",
      "id": "card_1_button_link",
      "label": "Knapplänk 1"
    },
    {
      "type": "range",
      "id": "card_1_overlay_opacity",
      "label": "Överlagring opacitet 1",
      "min": 0,
      "max": 100,
      "default": 0,
      "unit": "%"
    },
