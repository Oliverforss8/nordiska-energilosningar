{% liquid
  assign section_id = "header-" | append: section.id

  # Check for page-specific inverted mode
  assign inverted_mode = false

  # Default inverted mode for collection and product pages
  if template.name == "collection" or template.name == "product"
    assign inverted_mode = true
  endif

  # Define which pages should use inverted mode
  assign inverted_pages = section.settings.inverted_pages | split: ","

  # Check if current page/template should use inverted mode
  if template.name == "page" and page.handle != blank
    # Check if current page handle is in the inverted pages list
    for inverted_page in inverted_pages
      assign clean_page = inverted_page | strip
      if page.handle == clean_page
        assign inverted_mode = true
        break
      endif
    endfor
  elsif template.name != "page" and template.name != "collection" and template.name != "product"
    # Check if current template is in the inverted pages list (excluding collection/product which are already inverted)
    for inverted_page in inverted_pages
      assign clean_page = inverted_page | strip
      if template.name == clean_page
        assign inverted_mode = true
        break
      endif
    endfor
  endif

  # Also check global setting as fallback
  if section.settings.inverted_mode_global == true
    assign inverted_mode = true
  endif

  # Check if we're on a company/foretag page
  assign is_foretag_page = false
  if page.handle contains "foretag" or template.name contains "foretag" or request.page_type contains "foretag"
    assign is_foretag_page = true
  endif
%}

<header
  id='{{ section_id }}'
  class='header-section fixed w-full z-[100] transition-all duration-300{% if inverted_mode %} inverted{% endif %}'
  data-header
>
  <!-- Top bar -->
  <div class='header-topbar bg-[#F3F1E8] px-[50px] w-full py-[15px]'>
    <div class='flex text text-[#171717] flex-row gap-[15px] px-[50px]'>
      {% if is_foretag_page %}
        <a
          href='{{ routes.root_url }}'
          class='text-[12px] font-[300] text-[#171717] hover:opacity-70 transition-opacity'
        >
          {{ "general.privatperson" | t | default: "Privatperson" }}
        </a>
        <a
          href='/pages/foretag/start'
          class='text-[18px] font-[400] text-[#171717] hover:opacity-70 transition-opacity'
        >
          {{ "general.foretag" | t | default: "Företag" }}
        </a>
      {% else %}
        <a
          href='{{ routes.root_url }}'
          class='text-[18px] font-[400] text-[#171717] hover:opacity-70 transition-opacity'
        >
          {{ "general.privatperson" | t | default: "Privatperson" }}
        </a>
        <a
          href='/pages/foretag/start'
          class='text-[12px] font-[300] text-[#171717] hover:opacity-70 transition-opacity'
        >
          {{ "general.foretag" | t | default: "Företag" }}
        </a>
      {% endif %}
    </div>
  </div>

  <!-- Main header -->
  <div class='header-main py-[15px] w-full header-gradient'>
    <div class='layout w-full flex flex-row justify-between items-center'>
      <!-- Logo -->
      <a href='{{ routes.root_url }}' class='header-logo'>
        <img
          src='{{ 'logga-vit.svg' | asset_url }}'
          alt='{{ shop.name }}'
          width='auto'
          height='53'
          class='logo-white w-auto h-[53px] transition-opacity duration-300'
        >
        <img
          src='{{ 'logga.svg' | asset_url }}'
          alt='{{ shop.name }}'
          width='auto'
          height='53'
          class='logo-dark w-auto h-[53px] transition-opacity duration-300 opacity-0 absolute top-0 left-0'
        >
      </a>

      <!-- Navigation -->
      <nav class='header-nav flex text-white flex-row gap-[50px]'>
        {% for link in section.settings.menu.links %}
          {% liquid
            assign has_dropdown = false
            assign dropdown_image = blank
            assign dropdown_image_2 = blank
            assign button_link_1 = blank
            assign button_link_2 = blank
            assign button_text_1 = blank
            assign button_text_2 = blank

            if link.links.size > 0
              assign has_dropdown = true
              case forloop.index
                when 1
                  assign dropdown_image = section.settings.nav_item_1_dropdown_image
                  assign dropdown_image_2 = section.settings.nav_item_1_dropdown_image_2
                  assign button_link_1 = section.settings.nav_item_1_button_link_1
                  assign button_link_2 = section.settings.nav_item_1_button_link_2
                  assign button_text_1 = section.settings.nav_item_1_button_text_1
                  assign button_text_2 = section.settings.nav_item_1_button_text_2
                when 2
                  assign dropdown_image = section.settings.nav_item_2_dropdown_image
                  assign dropdown_image_2 = section.settings.nav_item_2_dropdown_image_2
                  assign button_link_1 = section.settings.nav_item_2_button_link_1
                  assign button_link_2 = section.settings.nav_item_2_button_link_2
                  assign button_text_1 = section.settings.nav_item_2_button_text_1
                  assign button_text_2 = section.settings.nav_item_2_button_text_2
                when 3
                  assign dropdown_image = section.settings.nav_item_3_dropdown_image
                  assign dropdown_image_2 = section.settings.nav_item_3_dropdown_image_2
                  assign button_link_1 = section.settings.nav_item_3_button_link_1
                  assign button_link_2 = section.settings.nav_item_3_button_link_2
                  assign button_text_1 = section.settings.nav_item_3_button_text_1
                  assign button_text_2 = section.settings.nav_item_3_button_text_2
                when 4
                  assign dropdown_image = section.settings.nav_item_4_dropdown_image
                  assign dropdown_image_2 = section.settings.nav_item_4_dropdown_image_2
                  assign button_link_1 = section.settings.nav_item_4_button_link_1
                  assign button_link_2 = section.settings.nav_item_4_button_link_2
                  assign button_text_1 = section.settings.nav_item_4_button_text_1
                  assign button_text_2 = section.settings.nav_item_4_button_text_2
              endcase
            endif
          %}

          {% if has_dropdown %}
            <div class='nav-item  dropdown-item relative'>
              <a
                href='{{ link.url }}'
                class='nav-link header-text hover:opacity-80 transition-opacity flex items-center gap-[8px]'
              >
                {{ link.title }}
                <svg
                  xmlns='http://www.w3.org/2000/svg'
                  width='14'
                  height='9'
                  viewBox='0 0 14 9'
                  fill='none'
                  class='dropdown-arrow'
                >
                  <path d="M0.344765 0.837496C0.235521 0.943564 0.148811 1.06976 0.0896384 1.20879C0.0304655 1.34783 0 1.49696 0 1.64758C0 1.7982 0.0304655 1.94733 0.0896384 2.08637C0.148811 2.22541 0.235521 2.3516 0.344765 2.45767L6.17247 8.1625C6.28082 8.26944 6.40973 8.35433 6.55176 8.41225C6.69379 8.47018 6.84614 8.5 7 8.5C7.15387 8.5 7.30621 8.47018 7.44824 8.41225C7.59027 8.35433 7.71918 8.26944 7.82753 8.1625L13.6552 2.45767C13.7645 2.3516 13.8512 2.22541 13.9104 2.08637C13.9695 1.94733 14 1.7982 14 1.64758C14 1.49696 13.9695 1.34783 13.9104 1.20879C13.8512 1.06976 13.7645 0.943564 13.6552 0.837496C13.5469 0.730556 13.418 0.645674 13.2759 0.587749C13.1339 0.529823 12.9816 0.5 12.8277 0.5C12.6738 0.5 12.5215 0.529823 12.3795 0.587749C12.2374 0.645674 12.1085 0.730556 12.0002 0.837496L7 5.74365L1.99983 0.837496C1.89148 0.730556 1.76257 0.645674 1.62054 0.587749C1.47851 0.529823 1.32616 0.5 1.1723 0.5C1.01843 0.5 0.866091 0.529823 0.724059 0.587749C0.582027 0.645674 0.453117 0.730556 0.344765 0.837496Z" fill="currentColor"/>
                </svg>
              </a>

              <!-- Custom Dropdown -->
              <div class='mega-dropdown pt-[35px] fixed top-[100px] right-0 left-0 w-screen opacity-0 invisible transition-all duration-300 z-50 pointer-events-none'>
                <div class='layout mx-auto pointer-events-auto'>
                  <div class='py-[40px] shadow-bg px-[50px] rounded-[15px] grid gap-[30px] grid-cols-2 bg-white w-full '>
                    <!-- Left Column - Categories -->
                    <div class='flex flex-col'>
                      <div class='headline1 text-[50px] text-[#171717] pb-[40px]'>
                        {{ link.title }}
                      </div>
                      <div class='flex flex-col gap-[10px]'>
                        {% for child_link in link.links %}
                          <a
                            href='{{ child_link.url }}'
                            class='dropdown-category flex flex-row p-[18px] rounded-[15px] justify-between text-[24px] headline1 transition-colors text-[#171717] w-full {% if forloop.first %}bg-[#F3F1E8]{% else %}bg-white{% endif %}'
                            data-category-item
                          >
                            {{ child_link.title }}
                            <div>
                              <svg
                                xmlns='http://www.w3.org/2000/svg'
                                width='23'
                                height='18'
                                viewBox='0 0 23 18'
                                fill='none'
                              >
                                <path d="M1.55554 9H22L14 1" stroke="#171717" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                <path d="M1.55554 8.99994H22L14 16.9999" stroke="#171717" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                              </svg>
                            </div>
                          </a>
                        {% endfor %}
                      </div>
                    </div>

                    <!-- Right Column - Image(s) -->
                    <div class='flex items-center h-full justify-center'>
                      {% if dropdown_image != blank
                        or dropdown_image_2 != blank
                      %}
                        {% liquid
                          assign has_two_images = false
                          if dropdown_image != blank and dropdown_image_2 != blank
                            assign has_two_images = true
                          endif
                        %}

                        {% if has_two_images %}
                          <!-- Two images side by side -->
                          <div class='flex flex-row h-full gap-[30px] w-full'>
                            <div class='flex h-full relative'>
                              <img
                                src='{{ dropdown_image | image_url: width: 600 }}'
                                alt='{{ link.title }}'
                                width='600'
                                height='400'
                                class='dropdown-image h-full object-cover w-full rounded-[10px]'
                                loading='lazy'
                              >
                              <div class='absolute bottom-[20px] left-[20px] '>
                                <a
                                  href='{{ button_link_1 }}'
                                  class='px-[20px] flex flex-row gap-[20px] items-center rounded-[10px] py-[17px] knappetikett text-[#171717] shrink-0'
                                  style='background: linear-gradient(0deg, rgba(255, 255, 255, 0.20) 0%, rgba(255, 255, 255, 0.20) 100%), #EBCF2F; text-decoration: none;'
                                >
                                  {{ button_text_1 | default: "Knapp" }}
                                  <svg
                                    xmlns='http://www.w3.org/2000/svg'
                                    width='22'
                                    height='18'
                                    viewBox='0 0 22 18'
                                    fill='none'
                                  >
                                    <path d="M1 8.99991H21L13.1739 1.17383" stroke="#171717" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                    <path d="M1 9.00009H21L13.1739 16.8262" stroke="#171717" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                  </svg>
                                </a>
                              </div>
                            </div>
                            <div class='flex h-full relative'>
                              <img
                                src='{{ dropdown_image_2 | image_url: width: 600 }}'
                                alt='{{ link.title }}'
                                width='600'
                                height='400'
                                class='dropdown-image h-full object-cover w-full rounded-[10px]'
                                loading='lazy'
                              >
                              <div class='absolute bottom-[20px] left-[20px] '>
                                <a
                                  href='{{ button_link_2 }}'
                                  class='px-[20px] flex flex-row gap-[20px] items-center rounded-[10px] py-[17px] knappetikett text-[#171717] shrink-0'
                                  style='background: linear-gradient(0deg, rgba(255, 255, 255, 0.20) 0%, rgba(255, 255, 255, 0.20) 100%), #EBCF2F; text-decoration: none;'
                                >
                                  {{ button_text_2 | default: "Knapp" }}
                                  <svg
                                    xmlns='http://www.w3.org/2000/svg'
                                    width='22'
                                    height='18'
                                    viewBox='0 0 22 18'
                                    fill='none'
                                  >
                                    <path d="M1 8.99991H21L13.1739 1.17383" stroke="#171717" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                    <path d="M1 9.00009H21L13.1739 16.8262" stroke="#171717" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                  </svg>
                                </a>
                              </div>
                            </div>
                          </div>
                        {% else %}
                          <!-- Single image -->
                          <div class='relative h-full flex w-full'>
                            {% assign single_image = dropdown_image %}
                            {% if dropdown_image == blank %}
                              {% assign single_image = dropdown_image_2 %}
                            {% endif %}
                            <img
                              src='{{ single_image | image_url: width: 1200 }}'
                              alt='{{ link.title }}'
                              width='1200'
                              height='800'
                              class='dropdown-image h-full w-full rounded-[10px]'
                              loading='lazy'
                            >
                            <div class='absolute bottom-[20px] left-[20px] '>
                              <a
                                href='{{ button_link_1 }}'
                                class='px-[20px] flex flex-row gap-[20px] items-center rounded-[10px] py-[17px] knappetikett text-[#171717] shrink-0'
                                style='background: linear-gradient(0deg, rgba(255, 255, 255, 0.20) 0%, rgba(255, 255, 255, 0.20) 100%), #EBCF2F; text-decoration: none;'
                              >
                                {{ button_text_1 | default: "Knapp" }}
                                <svg
                                  xmlns='http://www.w3.org/2000/svg'
                                  width='22'
                                  height='18'
                                  viewBox='0 0 22 18'
                                  fill='none'
                                >
                                  <path d="M1 8.99991H21L13.1739 1.17383" stroke="#171717" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                  <path d="M1 9.00009H21L13.1739 16.8262" stroke="#171717" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                </svg>
                              </a>
                            </div>
                          </div>
                        {% endif %}
                      {% endif %}
                    </div>
                  </div>
                </div>
              </div>
            </div>
          {% else %}
            <a
              href='{{ link.url }}'
              class='nav-link header-text hover:opacity-80 transition-opacity'
            >
              {{ link.title }}
            </a>
          {% endif %}
        {% endfor %}
      </nav>

      <!-- Cart button -->
      <button
        class='cart-btn flex flex-row gap-[20px] bg-white knappetikett text-[#171717] p-[20px] py-[15px] rounded-[10px] hover:bg-gray-100 transition-colors relative'
        data-cart-toggle
      >
        <span class='xl:block hidden '>kundvagn</span>
        <svg
          xmlns='http://www.w3.org/2000/svg'
          width='21'
          height='22'
          viewBox='0 0 21 22'
          fill='none'
        >
          <path d="M2.37325 16.02L1.16926 8.84C0.98726 7.754 0.896261 7.212 1.18826 6.856C1.47926 6.5 2.01525 6.5 3.08624 6.5H17.9141C18.9851 6.5 19.5211 6.5 19.8121 6.856C20.1041 7.212 20.0121 7.754 19.8311 8.84L18.6271 16.02C18.2281 18.4 18.0291 19.589 17.2141 20.295C16.4001 21 15.2261 21 12.8782 21H8.12219C5.77422 21 4.60023 21 3.78623 20.294C2.97124 19.589 2.77224 18.399 2.37325 16.019M16.0001 6.5C16.0001 5.04131 15.4207 3.64236 14.3892 2.61091C13.3578 1.57946 11.9588 1 10.5002 1C9.0415 1 7.64256 1.57946 6.61112 2.61091C5.57968 3.64236 5.00022 5.04131 5.00022 6.5" stroke="#171717" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
        </svg>
        <!-- Cart count badge -->
        <span
          class='cart-count absolute -top-2 -right-2 bg-[#171717] text-white text-xs rounded-full w-6 h-6 flex items-center justify-center font-bold opacity-0 scale-0 transition-all duration-200'
          data-cart-count
        >
          0
        </span>
      </button>
    </div>
  </div>
</header>

<!-- Include Cart Drawer -->
{% render "cart-drawer" %}

<!-- Load Cart JavaScript -->
<script src='{{ 'cart.js' | asset_url }}' defer></script>

<!-- Load Dropdown JavaScript -->
<script src='{{ 'dropdown.js' | asset_url }}' defer></script>

<!-- Load Header Scroll JavaScript -->
<script src='{{ 'header-scroll.js' | asset_url }}' defer></script>

{% stylesheet %}
  .header-section {
    --header-text-color: white;
    --header-bg: linear-gradient(0deg, rgba(23, 23, 23, 0) 0%, #171717 100%);
    --logo-white-opacity: 1;
    --logo-dark-opacity: 0;
  }

  .header-section.scrolled {
    --header-text-color: #171717;
    --header-bg: white;
    --logo-white-opacity: 0;
    --logo-dark-opacity: 1;
  }

  /* Ensure white background appears on scroll */
  .header-section.scrolled .header-gradient {
    background: white !important;
  }

  /* Scrolled state gets same cart button styling as inverted mode */
  .header-section.scrolled .cart-btn {
    background:
      linear-gradient(
        0deg,
        rgba(255, 255, 255, 0.2) 0%,
        rgba(255, 255, 255, 0.2) 100%
      ),
      #ebcf2f;
    color: #171717;
  }

  .header-section.scrolled .cart-btn svg path {
    stroke: #171717;
  }

  .header-section.scrolled .cart-btn:hover {
    background:
      linear-gradient(
        0deg,
        rgba(255, 255, 255, 0.3) 0%,
        rgba(255, 255, 255, 0.3) 100%
      ),
      #ebcf2f;
  }

  /* Inverted mode - dark text, no gradient */
  .header-section.inverted {
    --header-text-color: #171717;
    --header-bg: white;
    --logo-white-opacity: 0;
    --logo-dark-opacity: 1;
  }

  /* When inverted mode is active, scrolled state keeps the same styling */
  .header-section.inverted.scrolled {
    --header-text-color: #171717;
    --header-bg: white;
    --logo-white-opacity: 0;
    --logo-dark-opacity: 1;
  }

  .header-gradient {
    background: var(--header-bg);
  }

  .header-nav {
    color: var(--header-text-color);
  }

  .header-text {
    color: var(--header-text-color) !important;
  }

  .dropdown-arrow {
    color: var(--header-text-color);
  }

  .logo-white {
    opacity: var(--logo-white-opacity);
  }

  .logo-dark {
    opacity: var(--logo-dark-opacity);
  }

  .header-logo {
    position: relative;
  }

  .cart-count.show {
    opacity: 1;
    scale: 1;
  }

  /* Cart button styling for inverted mode */
  .header-section.inverted .cart-btn {
    background:
      linear-gradient(
        0deg,
        rgba(255, 255, 255, 0.2) 0%,
        rgba(255, 255, 255, 0.2) 100%
      ),
      #ebcf2f;
    color: #171717;
  }

  .header-section.inverted .cart-btn svg path {
    stroke: #171717;
  }

  .header-section.inverted .cart-btn:hover {
    background:
      linear-gradient(
        0deg,
        rgba(255, 255, 255, 0.3) 0%,
        rgba(255, 255, 255, 0.3) 100%
      ),
      #ebcf2f;
  }

  /* Mega Dropdown Styles */
  .dropdown-item:hover .mega-dropdown {
    opacity: 1;
    visibility: visible;
  }

  /* Adjust dropdown padding when header is scrolled */
  .header-section.scrolled .mega-dropdown {
    padding-top: 50px;
  }

  .dropdown-image {
    transition: transform 0.3s ease;
  }

  /* Ensure dropdown category backgrounds work properly */
  .dropdown-category[class*='bg-[#F3F1E8]'] {
    background-color: #f3f1e8 !important;
  }

  .dropdown-category[class*='bg-white'] {
    background-color: white !important;
  }

  /* Mobile responsive */
  @media (max-width: 768px) {
    .header-topbar {
      padding-left: 20px;
      padding-right: 20px;
    }

    .header-topbar .flex {
      padding-left: 20px;
      padding-right: 20px;
    }

    .header-nav {
      gap: 25px;
      font-size: 14px;
    }

    .layout {
      padding-left: 20px;
      padding-right: 20px;
    }
  }

  @media (max-width: 640px) {
    .header-nav {
      display: none;
    }

    .cart-btn {
      padding: 15px;
      gap: 15px;
    }

    .mega-dropdown {
      display: none;
    }
  }
{% endstylesheet %}

{% comment %}
  Header scroll functionality is handled in header-scroll.js
  Dropdown interactions are handled in dropdown.js
{% endcomment %}

